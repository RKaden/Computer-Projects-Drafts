public abstract class Food {
  
  public static void main(String[] args) {
     Food crab = new Crab();
     Food bacon = new Bacon();
     Food hamburger = new Hamburger();
     
     ArrayList<Food> foods = new ArrayList<Food>()
     foods.add(crab);
     foods.add(bacon);
     foods.add(hamburger);
  }
}

public class Crab extends Food {
  
  public void taste() {
    System.out.println("Buttery Ocean");
  }
}

public class Bacon extends Food {
  
  public void taste() {
    System.out.println("Greasily unhealthy");
  }
}

public class Hamburger extends Food {
  
  public void taste() {
    System.out.println("Perfect Combo");
  }
}
