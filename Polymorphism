public abstract class CardGame {
  
  public static void main(String[] args) {
  
  }
}

public class Magic extends CardGame {
  
  public void bestCard() {
    System.out.println("NicolBolas");
  }
}

public class Yugi-Oh extends CardGames {
  
  public void bestCard() {
    system.out.println("Blue eyes White Dragon!");
  }
}
